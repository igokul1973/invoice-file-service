{
  "name": "express-typescript-docker",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "dev:debug": "tsx watch --inspect=0.0.0.0:5680 ./src/index.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@pdfme/common": "^4.0.1",
    "@pdfme/generator": "^4.0.1",
    "@pdfme/schemas": "^4.0.1",
    "dotenv": "16.4.5",
    "express": "^4.19.2",
    "get-stream": "^8.0.1",
    "minio": "^8.0.0",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.15.0",
    "pdfmake": "^0.2.10",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/node": "^18.2.4",
    "@types/pdfkit": "^0.13.4",
    "@types/pdfmake": "^0.2.9",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  }
}